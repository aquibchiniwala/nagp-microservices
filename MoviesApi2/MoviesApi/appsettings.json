{
  "spring": {
    "application": {
      "name": "MoviesApi"
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "serviceUrl": "http://localhost:8761/eureka/",
      "ValidateCertificates": false
    },
    "instance": {
      "hostName": "localhost",
        "port": "72580",
        "instanceID" :  "Movie2"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
